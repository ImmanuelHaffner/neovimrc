{
  "mcpServers": {
    "github_personal": {
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "$$$"
      },
      "command": "docker",
      "args": [
        "run",
        "-i",
        "--rm",
        "-e",
        "GITHUB_PERSONAL_ACCESS_TOKEN",
        "ghcr.io/github/github-mcp-server"
      ],
      "disabled": false
    },
    "github_firebolt": {
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "$$$"
      },
      "command": "docker",
      "args": [
        "run",
        "-i",
        "--rm",
        "-e",
        "GITHUB_PERSONAL_ACCESS_TOKEN",
        "ghcr.io/github/github-mcp-server"
      ],
      "disabled": false
    },
    "github.com/modelcontextprotocol/servers/tree/main/src/git": {
      "args": [
        "mcp-server-git"
      ],
      "command": "uvx"
    },
    {
        "mcpServers": {
            "vectorcode-mcp-server": {
                "command": "vectorcode-mcp-server",
                "args": []
            }
        }
    }
  },
  "nativeMCPServers": [
    
  ]
}
